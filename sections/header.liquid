        },
        {
          "type": "header",
          "content": "t:sections.header.blocks.tabbed_menu.settings.label.content"
        },
        {
          "type": "text",
          "id": "label",
          "label": "t:sections.header.blocks.tabbed_menu.settings.label.label"
        },
        {
          "type": "color",
          "id": "label_color",
          "default": "#CE021E",
          "label": "t:sections.header.blocks.tabbed_menu.settings.label_color.label"
        },
        {
          "type": "color",
          "id": "label_text_color_light",
          "default": "#FFFFFF",
          "label": "t:sections.header.blocks.tabbed_menu.settings.label_text_color_light.label"
        },
        {
          "type": "color",
          "id": "label_color_dark",
          "default": "#CE021E",
          "label": "t:sections.header.blocks.tabbed_menu.settings.label_color_dark.label"
        },
        {
          "type": "color",
          "id": "label_text_color_dark",
          "default": "#FFFFFF",
          "label": "t:sections.header.blocks.tabbed_menu.settings.label_text_color_dark.label"
        },
        {
