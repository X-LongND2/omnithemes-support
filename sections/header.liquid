{%- liquid
  assign areas = "heading navigation icons"
  assign areas2 =  ". . ."
  assign class_grid = "col-span-full col-start-1 order-1 lg:mb-3"
  assign class = 'w-full justify-end'
  assign logo_above = false
  assign logo_center = false
  assign columns = 'auto 1fr auto'
  if section.settings.logo_position == 'above-center'
    assign areas =  "iconsRight heading icons" 
    assign areas2 =  "navigation navigation navigation"
    assign class_grid = '[grid-area:iconsRight] hidden lg:block'
    assign class = 'w-full'
    assign columns = '4fr 2fr 4fr'
    assign logo_above = true
  endif
  if section.settings.logo_position == 'left-center'
    assign columns = '1fr 4fr 1fr'
  endif
  if section.settings.logo_position == 'center-left'
    assign columns = '4fr 2fr 4fr'
    assign areas = ". . ."
    assign areas2 =  "navigation heading icons"
    assign logo_center = true
  endif
  assign transparent = false
  if section.settings.enable_on_homepage and template.name == "index"
    assign transparent = true
  endif
  if section.settings.enable_on_collection_page and template.name == "collection"
    assign transparent = true
  endif
  assign class_logo = "w-[" | append: section.settings.logo_width_mobile | append: "px] md:w-[" | append: section.settings.logo_width | append: "px] h-auto"
  assign logo_name = settings.heading_base_size | times: 0.0225
  if section.settings.header_show_currency_selector or section.settings.show_social_icons or section.settings.header_show_language_selector
    assign headerTop = true 
  else 
    assign headerTop = false
  endif
-%}
{%- style -%}
  {% if section.settings.sticky_header_type == "none" or section.settings.sticky_header_type == "on-scroll-up" %}
    :root{
      --height-header: 0px;
    }
  {% endif %}
  {%- if transparent -%}
    section:first-child {
      margin-top: 0;
    }
    #sticky-header-content {
      background: linear-gradient(to top, transparent 0%, rgba({{ section.settings.shadow_color.red }},{{ section.settings.shadow_color.green }},{{ section.settings.shadow_color.blue }}, {{ section.settings.shadow }}%) 100%);
    }
    #sticky-header-content.background-header {
      background: rgb(var(--background-color-header));
    }
  {%- endif -%}
  .logo-name {
    font-size: {{ logo_name | times: 0.5 }}rem;
  }
  {% unless section.settings.text_color_light.alpha == 0.0 %}
    .transparent-header .logo-name,
    .transparent-header header {
      color: {{ section.settings.text_color_light }};
    }
  {% endunless %}
  .dark .transparent-header .logo-name,
  .dark .transparent-header header {
    {% unless section.settings.text_color_dark.alpha == 0.0 %}
      color: {{ section.settings.text_color_dark }};
    {% else %}
      color: rgba(var(--colors-text));
    {% endunless %}
  }
  {% if section.settings.sticky_header_type == "reduce-logo-size" %}
    .reduce-logo-size .header-logo {
      width: 75%;
    }
  {% endif %}
  @media (min-width: 1024px) {
    header {
      grid-template-areas: "{{ areas }}" "{{ areas2 }}";
      grid-template-columns: {{ columns }};
    } 
    .logo-name {
      font-size: {{ logo_name }}rem;
    }
    .pt-header {
      padding-top: {{ section.settings.padding_top }}px;
    }
    .pb-header {
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
    {%- if transparent -%}
      #sticky-header-content:hover {
        background: rgba(var(--background-color-header))
      }
      #sticky-header-content:hover .divider {
        -colors-line-and-border: var(--background-color-header);
        --colors-line-header: var(--background-color-header);
      }
    {% endif %}
  }
{%- endstyle -%} 
<div
  id="x-header-container"
  class="relative text-[rgb(var(--colors-text-header))] z-50"
  x-data
  x-init='$store.xHeaderMenu.initSticky($el, "shopify-section-{{ section.id }}", "{{ section.settings.sticky_header_type }}")'
  {% if section.settings.sticky_header_type != "none" %}
    x-intersect:leave="$store.xHeaderMenu.handleAlwaysSticky()"
  {% endif %}
  x-intersect="$store.xHeaderMenu.removeStickyHeader()"
  {% if section.settings.sticky_header_type == 'on-scroll-up' %}@scroll.window="$store.xHeaderMenu.handelOnScrollSticky();"{% endif %}
>
  <div id="sticky-header" class="shopify-header text-[rgb(var(--colors-text-header))]{% if transparent %} transparent-header{% else %} background-header{% endif %}"
    {% if section.settings.sticky_header_type == 'always' or section.settings.sticky_header_type == 'reduce-logo-size' %}x-intersect="$store.xHeaderMenu.setVariableHeightHeader(true)"{% endif %}
  >
    <div 
      id="sticky-header-content"
      class="sticky-header-content w-full pt-[{{ section.settings.padding_top_mobile }}px] pb-[{{ section.settings.padding_bottom_mobile }}px] pt-header top-0 lg:pb-0{% if transparent %} absolute disable-tranparent-collection lg:hover:bg-[rgba(var(--background-color-header))]{% endif %}"
      :class="{ 'background-header sticky-header-active': $store.xHeaderMenu.isSticky, '{% unless headerTop %} lg:pt-5{% endunless %}': !$store.xHeaderMenu.isSticky, 'overflow-hidden padding-scrollbar': $store.xHeaderMenu.isSticky && $store.xPopup.open}"
      {% if transparent %}
        x-on:mouseover="$store.xHeaderMenu.addTransparentHover($el); $store.xHeaderMenu.showLogoTransparent = false"
        @mouseleave="!$store.xHeaderMenu.isSticky && $store.xHeaderMenu.removeTransparentHover($el); $store.xHeaderMenu.showLogoTransparent=true"
      {% endif %}
      x-intersect.once="$store.xHeaderMenu.setTopStickyHeader()"
    >
      <header class="header{% if section.settings.full_width %} full-width{% else %} page-width{% endif %} grid lg:mx-auto gap-x-2{% if logo_above %} items-center{% endif %}" :class="{ 'lg:pb-0': $store.xHeaderMenu.isSticky }">
        <div data-breakpoint="tablet" class="[grid-area:drawer] lg:hidden flex items-center md:w-32">
          <div @click="$store.xMobileNav.open()" class="relative -left-3 w-11 h-8 pl-3 pr-3 pt-2.5 pb-2.5 rounded-full cursor-pointer opacity-70"> 
            {% render 'icon-alls', icon: 'icon-hamburger' %}
          </div>
        </div>
        
        {%- if section.settings.header_show_language_selector or section.settings.show_social_icons or section.settings.header_show_currency_selector -%}
          <div class="{{ class_grid }} {{ class }} lg:gap-3" :class="{ 'max-h-0 opacity-0 lg:mb-0 overflow-hidden': $store.xHeaderMenu.isSticky }">
            <div class="hidden relative lg:block">
              {% render 'toolbar', 
                show_currency_selector: section.settings.header_show_currency_selector, 
                show_language_selector: section.settings.header_show_language_selector, 
                show_social_icons: section.settings.show_social_icons,
                logo_above: logo_above,
                header: true
              %}
            </div>
          </div>
        {%- endif -%} 

        <{% if request.page_type == 'index' %}h1 class="flex {% else %}div class="{% endif %}logo-name p-break-words lg:order-2 top-4 justify-center text-center [grid-area:heading] mx-auto{% if logo_above == false %} pb-header flex items-center lg:justify-left lg:relative lg:left-0 lg:translate-x-0 lg:mt-0 lg:top-0{% unless logo_center %} lg:[grid-area:auto] lg:ml-0{% endunless %} {% else %} lg:mx-auto lg:pb-0{% endif %}">
          {% if section.settings.logo_transparent and transparent %}
            {%- render 'header-logo-transparent', class_logo: class_logo, logo_above: logo_above, transparent: transparent, logo_transparent: section.settings.logo_transparent -%}
          {% else %}
            {%- render 'header-logo-block', class_logo: class_logo, logo_above: logo_above, transparent: transparent -%}
          {% endif %}
        {%- if request.page_type == 'index' -%}
          </h1>
        {% else %}
          </div>
        {%- endif -%}

        {%- render 'header-megamenu-block', logo_above: logo_above, logo_center: logo_center -%}
        
        <div
          class="flex justify-end order-4 [grid-area:icons]{% if logo_above %} lg:[grid-area:icons]{% else %} {% if logo_center %} lg:[grid-area:icons]{% else %} lg:[grid-area:auto]{% endif %} pb-header{% endif %}"
        >
          {%- if settings.search_enabled -%}
            {%- render 'header-search-block' -%}
          {%- endif -%}
          {% if section.settings.button_label != blank %}
            <div class="hidden md:flex items-center">
              {% if section.settings.button_link != blank %}
                <a href="{{ section.settings.button_link }}"
              {% else %}
                <p
              {% endif %} 
                  class="button button-solid empty:hidden pt-2.5 pb-2.5 pl-7 pr-7 md:pl-9 md:pr-9 md:pt-3 md:pb-3 leading-normal cursor-pointer inline-flex justify-center items-center{% if section.settings.button_link == blank %} hover:cursor-not-allowed opacity-70{% endif %}">{{ section.settings.button_label | escape }}
              {% if section.settings.button_link != blank %}
                </a>
              {% else %}
                </p>
              {% endif %}
            </div>
          {% endif %}
          {%- if shop.customer_accounts_enabled -%}
            <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
              aria-label="{{ 'customer.log_in' | t }}"
              class="header-icon-account hidden lg:flex disable-effect opacity-70 relative items-center justify-center disable-effect">
              <span class="w-11 h-10 pl-3 pr-2.5 pt-2.5 pb-2">
                {% render 'icon-alls', icon: 'icon-account' %}
              </span>
            </a>
          {%- endif -%}
          <div class="header-icon-cart cursor-pointer relative flex items-center mr-1">
            <a 
              id="cart-icon"
              href="{{ routes.cart_url }}"
              class="relative flex items-center disable-effect"
              @click.prevent="$store.xModal.setActiveElement('cart-icon'); $store.xMiniCart.openCart();"
              :class="$store.xMiniCart.open && 'pointer-events-none'"
              aria-controls="CartDrawer"
              aria-expanded="false"
              aria-label="icon-cart"
            >
              <span class="w-11 h-10 pl-3 inline-block pr-2.5 pt-2.5 pb-2 opacity-70">
                {% render 'icon-cart-all', icon: settings.cart_icon %}
              </span>
              <div id="cart-icon-bubble" class="count_item absolute -top-0.5 right-0">
                <div class="bg-[rgba(var(--colors-cart))] text-[rgba(var(--colors-cart-text))] font-medium h-5 w-5 rounded-full text-[10px]{% if cart == empty %} hidden{% endif %}">
                  {%- if cart.item_count < 100 -%}
                    <span class="h-full flex items-center justify-center mt-[1px]" aria-hidden="true">{{ cart.item_count }}</span>
                  {%- else -%}
                    <span class="h-full flex items-center justify-center mt-[1px]">99+</span> 
                  {%- endif -%}
                </div>
              </div>
            </a>
          </div>
           {%- if settings.theme_mode_enabled -%}
            {%- if settings.theme_mode_mode == 'switch' -%}
              <theme-mode x-data class="w-10 flex items-center justify-end lg:justify-center cursor-pointer ml-2">
                <button
                  @click.prevent="$store.xDarkMode.toggleThemeMode(); $store.xHeaderMenu.handleChangeThemeMode();"
                  class="button button-action rounded-full text-sm pt-2.5 pb-2.5 pl-2.5 pr-2.5"
                  aria-label="darkmode"
                >
                  <span class="w-4 lg:w-5 hidden dark:block">
                    {%- render 'icon-alls', icon: 'icon-dark' -%}
                  </span>
                  <span class="block w-4 lg:w-5 dark:hidden">
                    {%- render 'icon-alls', icon: 'icon-light' -%}
                  </span>
                </button>
              </theme-mode>
            {%- endif -%}
          {%- endif -%}
        </div>
      </header>
      {%- liquid 
        unless template == 'cart'
          render 'cart-drawer', transparent: transparent
        endunless
      -%}
      {% render 'section-divider', classHeader: ' absolute top-full left-0 w-full' %}
    </div>
  </div>
</div>
<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "Organization",
    "name": {{ shop.name | json }},
    {% if section.settings.logo %}
      "logo": {{ section.settings.logo | image_url: width: section.settings.logo.width | prepend: "https:" | json }},
    {% endif %}
    "sameAs": [
      {{ settings.social_twitter_link | json }},
      {{ settings.social_facebook_link | json }},
      {{ settings.social_pinterest_link | json }},
      {{ settings.social_instagram_link | json }},
      {{ settings.social_tiktok_link | json }},
      {{ settings.social_tumblr_link | json }},
      {{ settings.social_snapchat_link | json }},
      {{ settings.social_youtube_link | json }},
      {{ settings.social_vimeo_link | json }}
    ],
    "url": {{ request.origin | append: page.url | json }}
  }
</script>
{%- if request.page_type == 'index' -%}
  {% assign potential_action_target = request.origin | append: routes.search_url | append: "?q={search_term_string}" %}
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "WebSite",
      "name": {{ shop.name | json }},
      "potentialAction": {
        "@type": "SearchAction",
        "target": {{ potential_action_target | json }},
        "query-input": "required name=search_term_string"
      },
      "url": {{ request.origin | append: page.url | json }}
    }
  </script>
{%- endif -%}
{% schema %}
